<!-- TXP9 is encoded by (was used to encode) -->
	<rdf:Property rdf:about="TXP9_is_encoded_by">
		<rdfs:label xml:lang="en">is encoded by</rdfs:label>
		<rdfs:domain rdf:resource="TX1_Written_Text"/>
